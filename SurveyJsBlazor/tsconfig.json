{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "strict": true,
    "target": "ES2017",
    "paths": {
      "/_content/SurveyJsBlazor/scripts/*": [ "./wwwroot/scripts/*" ]
    },
    "types": [
      "knockout"
    ]
  },
  "exclude": [
    "node_modules",
    "wwwroot/libs"
  ]
}
