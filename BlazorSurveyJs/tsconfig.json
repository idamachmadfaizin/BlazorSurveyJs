{
  "compileOnSave": true,
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": false,
    "strict": true,
    "esModuleInterop": true,
    "target": "ES6",
    "module": "ES6"
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
